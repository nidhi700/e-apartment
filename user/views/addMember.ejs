<!DOCTYPE html>
<html lang="en">
<body class="animsition">
    <%- include('sidebar'); %>
        <div class="page-container">
            <!-- HEADER DESKTOP-->
            <div class="main-content">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">
                        <div class="row">                            
                            <div class="col-lg-12">
                                <div class="card">
                                    <div class="card-header">
                                        <strong>ADD MEMBERS</strong>                                        
                                    </div>
                                    <form class="form-header" action="/insertMember" method="POST">
                                    <div class="card-body card-block">     
                                        <div class="form-group">
                                            <label for="company" class=" form-control-label">Flat Number</label>
                                            <select name="flatno" id="flatno" class="form-control">
                                                <%
                                                if(data.length!=0){
                                                var i=1;
                                                data.forEach(function(res){
                                                %>
                                                    <option value="<%=res.Flat_No %>"><%=res.Flat_No %></option>
                                                    <!-- <option value="1">B-105</option>        -->
                                                <%}) %>
                                                <% } %>
                                                    <!-- <option value="1">No</option> -->
                                                </select>
                                        </div>
                                        <div class="form-group">
                                            <label for="vat" class=" form-control-label">Full Name</label>
                                            <input type="text" id="tenant_name" name="tenant_name" placeholder="Enter Full Name" class="form-control" title="Required * " required>
                                        </div>
                                        <div class="form-group">
                                            <label for="email" class=" form-control-label">Email_ID</label>
                                            <input type="email" id="email" name="email" placeholder="Enter Email ID" class="form-control" required title="Plz enter proper format..">
                                        </div> 
                                        <div class="form-group">
                                            <label for="street" class=" form-control-label">Number of Flat Members</label>
                                            <input type="text" id="t_flat_mem" name="flat_mem" placeholder="Enter total number of flat members" pattern="[0-9]*" title="Only Number are allowed" required class="form-control">
                                        </div>
                                        <div class="form-group">
                                            <label for="city" class=" form-control-label">Contact Number</label>
                                            <input type="text" id="t_contact" name="contact" placeholder="Enter Contact Number" class="form-control" maxLength="10"
                                                            required
                                                            pattern="[0-9]*"
                                                            title="Only Numbers are allowed.."
                                                            minLength="10">
                                        </div> 
                                        <div class="form-group">
                                                <label for="select" class=" form-control-label">Role</label>                                                
                                                <select name="role" id="role" class="form-control">
                                                        <option value="Owner">Owner</option>
                                                        <option value="Tenant">Tenant</option>
                                                </select>
                                        </div>                                   
                                        <div class="form-actions form-group">
                                                <button type="submit" class="btn btn-primary btn-sm">Add</button>
                                        </div></form>
                                    </div>
                                    </div>
                                </div>
                            </div>                        
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>

    <!-- Jquery JS-->
    <script src="vendor/jquery-3.2.1.min.js"></script>
    <!-- Bootstrap JS-->
    <script src="vendor/bootstrap-4.1/popper.min.js"></script>
    <script src="vendor/bootstrap-4.1/bootstrap.min.js"></script>
    <!-- Vendor JS       -->
    <script src="vendor/slick/slick.min.js">
    </script>
    <script src="vendor/wow/wow.min.js"></script>
    <script src="vendor/animsition/animsition.min.js"></script>
    <script src="vendor/bootstrap-progressbar/bootstrap-progressbar.min.js">
    </script>
    <script src="vendor/counter-up/jquery.waypoints.min.js"></script>
    <script src="vendor/counter-up/jquery.counterup.min.js">
    </script>
    <script src="vendor/circle-progress/circle-progress.min.js"></script>
    <script src="vendor/perfect-scrollbar/perfect-scrollbar.js"></script>
    <script src="vendor/chartjs/Chart.bundle.min.js"></script>
    <script src="vendor/select2/select2.min.js">
    </script>

    <!-- Main JS-->
    <script src="js/main.js"></script>

</body>

</html>
<!-- end document-->